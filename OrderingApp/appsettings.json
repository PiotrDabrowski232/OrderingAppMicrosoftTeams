{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "TeamsFx": {
    "Authentication": {
      "ClientId": "$clientId$",
      "ClientSecret": "$client-secret$",
      "InitiateLoginEndpoint": "$TAB_ENDPOINT$/auth-start.html",
      "OAuthAuthority": "$oauthAuthority$"
    }
  },
  "ConnectionStrings": {
    "Connection": "Server=tcp:orderingapp-server-db.database.windows.net,1433;Initial Catalog=SqlDborderingApp;Persist Security Info=False;User ID=PiotrDabrowski;Password=Pjoter232!;MultipleActiveResultSets=False;Encrypt=True;TrustServerCertificate=False;Connection Timeout=30;"
  },

  "Serilog": {
    "MinimumLevel": {
      "Default": "Error",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning"
      }
    },
    "WriteTo": [
      {
        "Name": "Console"
      },
      {
        "Name": "File",
        "Args": {
          "path": "logs/log-.txt",
          "rollingInterval": "Day"
        }
      }
    ]
  }
}
